{
    "name": "pw/laraongkir",
    "description": "Package untuk request API RajaOngkir, Support Starter, Basic, dan Pro",
    "keywords": [
        "rajaongkir", "application programming interface", "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Purwadi",
            "email": "purwadie97@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.2.0||~5.3.0||~5.4.0||~5.5.0||~5.6.0||~5.7.0",
        "illuminate/support": "5.5.x|5.6.x|5.7.x"
    },
    "autoload": {
        "psr-4": {
            "Pw\\RajaOngkir\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pw\\RajaOngkir\\RajaOngkirServiceProvider"
            ],
            "aliases": {
                "RajaOngkir": "Pw\\RajaOngkir\\Facades\\RajaOngkir"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
